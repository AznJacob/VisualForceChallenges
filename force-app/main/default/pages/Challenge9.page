<apex:page controller="AccountContactController">
    <apex:form>
        <apex:pageBlock>
            <apex:pageBlockTable value="{!accountWrappers}" var="currAcc">
                <apex:column headerValue="Account Name">
                    <apex:outputText value="{!currAcc.accName}" />
                </apex:column>
                <apex:column headerValue="Contact Names">
                    <apex:outputText value="{!currAcc.conNames}" />
                </apex:column>
            </apex:pageBlockTable>
        </apex:pageBlock>
    </apex:form>
</apex:page>